{
  "name": "trpc-fe-boilerplate-vite",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "trpc-api-import": "npx tsx scripts/trpc-api-import.ts",
    "prepare": "husky",
    "dev": "VITE_PUBLIC_TRPC_API=http://localhost:4000/trpc vite",
