    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "octokit": "4.1.3",
    "postcss": "8.4.49",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tailwindcss": "3.4.16",
    "typescript": "5.8.3",
