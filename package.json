    "@tanstack/router-devtools": "1.79.0",
    "@trpc/client": "10.45.2",
    "@trpc/react-query": "10.45.2",
    "@trpc/server": "10.45.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "superjson": "2.2.1",
    "trpc-api-boilerplate": "1.0.52",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "9.14.0",
    "@tanstack/router-vite-plugin": "1.79.0",
    "@types/eslint": "9.6.1",
