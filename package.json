    "@tanstack/react-router": "1.124.0",
    "@tanstack/react-router-devtools": "1.124.0",
    "@trpc/client": "11.4.3",
    "@trpc/server": "11.4.3",
    "@trpc/tanstack-react-query": "11.4.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "superjson": "2.2.2",
    "trpc-api-boilerplate": "1.0.57",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "9.30.1",
    "@tailwindcss/vite": "4.1.11",
