    "@tanstack/router-devtools": "1.15.17",
    "@trpc/client": "10.45.0",
    "@trpc/react-query": "10.45.0",
    "@trpc/server": "10.45.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "2.2.1",
    "trpc-api-boilerplate": "1.0.41",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@tanstack/router-vite-plugin": "1.15.18",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
