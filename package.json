    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.36.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.12",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "octokit": "4.0.2",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
